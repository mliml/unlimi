{
  "name": "unlimi-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:landing\" \"npm run dev:app\"",
    "dev:landing": "cd landing && npm run dev",
    "dev:app": "cd app && npm run dev",
    "build": "npm run build:landing && npm run build:app",
    "build:landing": "cd landing && npm run build",
    "build:app": "cd app && npm run build",
    "install:all": "cd landing && npm install && cd ../app && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
